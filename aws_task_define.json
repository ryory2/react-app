{
    "family": "react-task",
    "containerDefinitions": [
        {
            "name": "react-container",
            "image": "990606419933.dkr.ecr.ap-northeast-1.amazonaws.com/react-frontend",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "react-container-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/react-task",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        },
        {
            "name": "springboot-container",
            "image": "990606419933.dkr.ecr.ap-northeast-1.amazonaws.com/springboot-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "springboot-container-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::990606419933:role/ecsTackRole",
    "executionRoleArn": "arn:aws:iam::990606419933:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}